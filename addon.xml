<addon id="plugin.video.rightnow-media"
       version="0.1.0"
       name="RightNow Media"
       provider-name="__cc__">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <!--<import addon="script.module.requests" version="2.1.0"/>-->
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Unoffical RightNow Media Viewer</summary>
        <description>https://www.rightnowmedia.org/</description>
        <platform>all</platform>
        <license>GNU GENERAL PUBLIC LICENSE. Version 3, 29 June 2007</license>
        <email>carl@free.org.nz</email>
    </extension>
</addon>
